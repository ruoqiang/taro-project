 
  .title {
    width: 100%;
    padding: 60px 0 50px;
    text-align: center;
    color: #2d4ed1;
    font-size: 36px
}

.inputBox {
    padding: 0 56px;
    position: relative;
    color: #2d4ed1;
    font-size: 28px
}

.inputBox.padType2 {
    padding: 0 0 0 56px
}

.inputBox .input {
    padding: 0;
    height: 112px;
    line-height: 112px;
    text-align: left;
    position: relative
    display: flex
    align-items:center

}

.inputBox .input:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1PX solid #ddd;
    content: " ";
    width: 100%
}

.inputBox .input.size-2 {
    padding: 0;
    height: 49px;
    line-height: 49px
}

.inputBox .input.align-0 {
    height: 63px;
    line-height: 63px
}

.inputBox .input.align-1 {
    height: 56px;
    line-height: 56px
}

.inputBox .input.align-2 .label {
    display: inline!important;
    margin-right: 2em
}

.inputBox .input.textAlignRight input {
    float: right;
    margin-top: 30px;
    text-align: right;
    margin-right: 30px
}

.inputBox .input .label {
    
    // margin-right: 1em
    font-size: 28px;
    color: #333;
    min-width: 100px;
    display: inline-block;
    vertical-align: middle
}

.inputBox .input .inputt {
    color: #333;
    margin-top: -1px;
    background: none
    display:inline-block;
    width: auto;
    font-size: 28px;
    vertical-align: middle
}

.inputBox .input .inputt .large {
    width: 66%
}

.inputBox .input h2 {
    font-size: 16px;
    color: #333
}
.buttonBox {
    margin-top: 51px
}

.buttonBox.disable {
    pointer-events: none
}
.buttonBox .button.disable{
    pointer-events: none;
    background: #ddd
    }
.forget-register {
    margin: 25px 28px 0;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between
}

.forget-register a {
    -webkit-box-flex: 0;
    -ms-flex: 0 0 auto;
    flex: 0 0 auto;
    font-size: 13px;
    color: #666;
    display: block
}

.forget-register a.register-btn {
    color: #2d4ed1
}

