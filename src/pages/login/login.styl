}
.send-smg-code {
    position: absolute;
    right: 0;
    width: 142px;
    height: 46px;
    font-size: 24px;
    line-height: 50px;
    height: 50px;
    text-align: center;
    border: 1PX solid #2d4ed1;
    border-radius: 6px;
    top: 28px
    z-index:2
}

.send-smg-code.disable {
